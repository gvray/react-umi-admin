# 当前环境（可用于代码内判断环境）
APP_ENV=staging

# API 基地址（默认值，可被覆盖）
APP_API_URL=https://api.dev.example.com
APP_API_TOKEN_KEY=token_key
APP_API_TIMEOUT=60000

# 静态资源 CDN（用于配置 publicPath）
APP_CDN_URL=

# 构建版本号（CI/CD 注入）
APP_VERSION=$CI_COMMIT_SHA

# 构建时间（CI/CD 注入）
APP_BUILD_TIME=$CI_BUILD_TIME

# Sentry 错误监控（可为空）
APP_SENTRY_DSN=

# 功能开关（布尔值请用字符串）
# 本地模拟数据是否开启
APP_MOCK_ENABLED=false

# 埋点系统
APP_TRACKING_ID=